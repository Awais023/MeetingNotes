@model IEnumerable<ProjectsNotes.Models.AddParticipant>

@{
    ViewBag.Title = "ViewMembers";
}

<h2>ViewMembers</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Emails)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Emails)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AdditionNo }) |
            @Html.ActionLink("Details", "Details", new { id=item.AdditionNo }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AdditionNo })
        </td>
    </tr>
}

</table>
